SQUIRREL;

function Run(PNo)
{
  return Act_ShotL(PNo);
}

ENDSQUIRREL;

TS_ActionType = AT_SHOT;
'各方向用
'TS_ShotL = 102;
'TS_ShotR = 101;

'攻撃
IF(@int[151]  ,==,    0);
  @AttackFlag = 1;
  GetPartsInfo(0);
  IF(@NowActNo ,==, 22); 'BoostShot
    IF(@WDGauge2 ,&lt;, 1);
      'リロード
      ChangeParts(26,0,TRUE,1,0.1, 1);
      ChangeAnime(115,0,0,    1);
      TSD_ChangeAnime(-100);
      EXIT;
    ELSE;
      ChangeParts(26,0,TRUE,1,0.1, 1);
      ChangeAnime(108,0,0,    1);
      'ChangeScript(108,-1,    1);
      TSD_ChangeAnime(-100);
      'TSD_ChangeScript(-100);
      EXIT;
    ENDIF;
  ELSE;  'Normal Shot
    IF(@WDGauge2 ,&lt;, 1);
      'リロード
      ChangeParts(26,0,TRUE,1,0.1, 1);
      ChangeAnime(115,0,0,    1);
      TSD_ChangeAnime(-100);
      EXIT;
    ELSE;
      ChangeParts(26,0,TRUE,1,0.1, 1);
      ChangeAnime(104,0,0,    1);
  '    ChangeScript(104,-1,    1);

      TSD_ChangeAnime(-100);
      TSD_ChangeScript(-100);
      EXIT;
    ENDIF;
  ENDIF;
ENDIF;

 

